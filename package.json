{
  "name": "placm-backend",
  "version": "1.0.0",
  "description": "Back-end to PLACM application",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "clean": "node clean",
    "build": "npm run clean && tsc --build",
    "build:prod": "npm run clean && tsc --build",
    "start:dev": "ts-node-dev --respawn --transpileOnly ./index.ts",
    "start": "npm run build && NODE_ENV=production && node ./dist/index",
    "start:win": "npm run build && set NODE_ENV=production && node ./dist/index",
    "crawler": "npm run build && node ./dist/models/admin/crawler"
  },
  "keywords": [
    "placm",
    "website",
    "backend",
    "accessibility",
    "act"
  ],
  "author": "Bruno Andrade",
  "license": "ISC",
  "dependencies": {
    "@mysql/xdevapi": "8.0.19",
    "ansi-colors": "^4.1.1",
    "apify": "^1.2.0",
    "compression": "^1.7.4",
    "convert-excel-to-json": "^1.7.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "5.3.1",
    "jsonld": "^5.2.0",
    "lodash.clonedeep": "^4.5.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-html-parser": "^1.4.9",
    "puppeteer": "^2.1.1",
    "random-words": "^1.1.1",
    "simplecrawler": "^1.1.9",
    "ts-node-dev": "^1.1.6",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/dom-parser": "^0.1.1",
    "@types/express": "^4.17.11",
    "@types/jsonld": "^1.5.5",
    "@types/lodash": "^4.14.168",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/morgan": "^1.9.2",
    "@types/mysql": "^2.15.18",
    "@types/node": "^13.13.51",
    "@types/puppeteer": "^2.1.5",
    "@types/xmldom": "^0.1.30",
    "nodemon": "^2.0.7",
    "typescript": "^4.2.0"
  }
}
